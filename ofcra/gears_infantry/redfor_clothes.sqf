ofcra_fnc_get_redfor_clothes = {
	private ["_param","_result"];
	_param = _this select 0;

	switch (_param) do {
		case "VDV":	{
			_result = ofcra_redfor_clothes_vdv;
		};
		case "VDV-M":	{
			_result = ofcra_redfor_clothes_vdvm;
		};
		case "MSV_DIGITAL":	{
			_result = ofcra_redfor_clothes_msv_digital;
		};
		case "MSV_FLORA":	{
			_result = ofcra_redfor_clothes_msv_flora;
		};
		case "SPETZ":	{
			_result = ofcra_redfor_clothes_spetz;
		};
		case "M88":	{
			_result = ofcra_redfor_clothes_m88;
		};
		

		default {
			_log = "tenue REDFOR inconnue '" + _param + "'";
			[_log, "ERROR"] call ofcra_fnc_log;
		};
	};
	_result;
};
