ofcra_fnc_get_bluefor_clothes = {
	private ["_param","_result"];
	_param = _this select 0;
	_result = nil;
	
	switch (_param) do {
		case "UCP":	{
			_result = ofcra_bluefor_clothes_ucp;
		};
		case "OCP":	{
			_result = ofcra_bluefor_clothes_ocp;
		};
		case "SOC":	{
			_result = ofcra_bluefor_clothes_soc;
		};
		case "MARPAT_WOOD":	{
			_result = ofcra_bluefor_clothes_marpat_wd;
		};
		case "MARPAT_DESERT":	{
			_result = ofcra_bluefor_clothes_marpat_d;
		};
		default {
			_log = "tenue BLUEFOR inconnue '" + _param + "'";
			[_log, "ERROR"] call ofcra_fnc_log;
		};
	};
	_result;
};
